<div class="page-width">

  <div id="image_v_1" style="margin: 5px 0; padding: 5px; border: 1px solid brown;">
    <h2>Зображення варіант 1</h2>
    {% if section.settings.image_v_1 != blank %}
      <div>
        {{ section.settings.image_v_1 | image_url: width: 300 | image_tag: class: 'image_v_1', loading: 'lazy' }}
        {{ section.settings.image_v_1 | img_url: '100x' | img_tag }}
        {{ section.settings.image_v_1 | image_url: width: 200 | image_tag: alt: 'honey' }}
        {{ section.settings.image_v_1 | image_url: width: 400, height: 400, crop: 'center' | image_tag }}
